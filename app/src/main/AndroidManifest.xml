<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.hamza.finalyearproject1">

    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FrontActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <meta-data
                    android:name="com.facebook.sdk.ApplicationId"
                    android:value="@string/facebook_app_id"/>
                <meta-data
                    android:name="com.facebook.accountkit.ApplicationName"
                    android:value="@string/app_name"/>
                <meta-data
                    android:name="com.facebook.accountkit.ClientToken"
                    android:value="@string/ACCOUNT_KIT_CLIENT_TOKEN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ComplainSubmision">
        </activity>
        <activity android:name=".ComplainSubmisionAuthentication">
        </activity>
        <activity
            android:name=".AndroidDatabaseManager"
            android:theme="@style/Theme.AppCompat.Light">
        </activity>
        <activity android:name=".Login_Activity">
        </activity>
        <activity android:name=".CurrentStatus">
        </activity>
    </application>

</manifest>